{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "NODE_ENV=production node dist/index.js",
    "dev": "NODE_ENV=development npx tsc && node dist/index.js",
    "dev:emulator": "NODE_ENV=development firebase emulators:start --only firestore,auth,functions",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:unit": "NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "NODE_ENV=test jest --testPathPattern=integration",
    "test:e2e": "NODE_ENV=test jest --testPathPattern=e2e",
    "test:ci": "NODE_ENV=test firebase emulators:exec --only firestore,auth 'npm test'",
    "test:ur": "NODE_ENV=test jest --testNamePattern",
    "test:results": "node scripts/generate-test-results.js",
    "test:update": "./scripts/update-test-results.sh",
    "lint": "npx tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,js,json,yaml}\"",
    "check-format": "prettier --check \"src/**/*.{ts,js,json,yaml}\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.30",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@google-cloud/firestore": "^7.11.1",
    "@google-cloud/storage": "^7.16.0",
    "@google/generative-ai": "^0.24.1",
    "@types/axios": "^0.9.36",
    "axios": "^1.10.0",
    "cors": "^2.8.5",
    "dotenv": "^17.0.1",
    "express": "^4.21.2",
    "firebase-admin": "^13.4.0",
    "firebase-functions": "^6.3.2"
  }
}
